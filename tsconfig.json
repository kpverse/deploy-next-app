{
    "compilerOptions": {
        "target": "es2016",
        "module": "ES6",
        "rootDir": "./src/",
        "moduleResolution": "node",
        "outDir": "./build/converted/",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
